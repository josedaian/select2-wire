{
    "name": "josedaian/select2wire",
    "description": "Select2 for livewire components.",
    "type": "package",
    "keywords": [
        "select2 livewire",
        "laravel select2",
        "select2-livewire",
        "livewire",
        "livewire select"
    ],
    "homepage": "https://github.com/josedaian/select2wire",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Daian",
            "email": "josedaian.cabrera@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "livewire/livewire": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "livewire/livewire": "^3.0"
    },    
    "autoload": {
        "psr-4": {
            "JoseDaian\\Select2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoseDaian\\Select2\\Tests\\": "tests/"
        }
    },    
    "extra": {
        "laravel": {
            "providers": [
                "JoseDaian\\Select2\\Select2ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
